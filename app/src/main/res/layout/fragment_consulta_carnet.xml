<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#50B848"
    tools:context=".ConsultaCarnet" >

    <ImageButton
        android:id="@+id/imgbtMenu"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/btn_redondo"
        android:clickable="true"
        android:contentDescription="Menu de opciones para el alumno"
        android:src="@drawable/baseline_menu_24"
        tools:layout_editor_absoluteX="355dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imgbtRegresar"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/btn_redondo"
        android:src="@drawable/baseline_arrow_back_24"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="632dp" />

    <ImageButton
        android:id="@+id/imgbtScanner"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/btn_redondo"
        android:src="@drawable/baseline_arrow_back_24"
        tools:layout_editor_absoluteX="304dp"
        tools:layout_editor_absoluteY="632dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
